
@{
    ViewBag.Title = "Request 20";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<courseworkv5.Tickets>

<body>
    <div align="center">


        <h2>Вивести інформацію про повернені квитки.</h2>

        <h4>Квитки:</h4>
        <table class="table table-striped">
            <tr>
                <th>Ticket Id</th>
                <th>Worker Id</th>
                <th>Client Id</th>
                <th>Passage Id</th>
                <th>Time Of Sell</th>
                <th>Taken Back</th>
            </tr>
            <tbody>
                @{
                    foreach (var t in Model)
                    {
                        <tr>
                            <td>@t.TicketId</td>
                            <td>@t.WorkerId</td>
                            <td>@t.BuyerId</td>
                            <td>@t.PassageId</td>
                            <td>@t.TimeOfSell</td>
                            <td>@t.TakenBackTickets.First(c => c.TicketId == t.TicketId).TakenBackTicketsId</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="script.js"></script>

</body>

