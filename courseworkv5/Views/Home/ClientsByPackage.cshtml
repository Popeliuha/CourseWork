
@{
    ViewBag.Title = "Request 18";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<courseworkv5.Clients>

<body>
    <div align="center">


        <h2>Вивести інформацію про клієнтів за зданим багажем.</h2>

        <h4>Клієнти:</h4>
        <table class="table table-striped">
            <tr>
                <th>Client Id</th>
                <th>Client Name</th>
                <th>Adress</th>
                <th>Gender</th>
                <th>Given Package</th>
            </tr>
            <tbody>
                @{
                    foreach (var t in Model)
                    {
                        <tr>
                            <td>@t.ClientId</td>
                            <td>@t.ClientName</td>
                            <td>@t.Adress</td>
                            <td>@t.Sex</td>
                            <td>@t.GivenPackage</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="script.js"></script>

</body>

